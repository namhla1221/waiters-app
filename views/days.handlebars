<h1>Sinovuyo's Coffee Shop</h1>

<h3>Booked shifts</h3>

<div class="box ">
    <div class=" rows moveDown">
        <div class="two row">
            {{#each storedShifts}}
            <ul id="shiftdays" >
                <h6>{{day}}</h6>
                {{#each Waiters}}
                <li class="{{../colours}}">{{this}}</li>
                {{/each}}
            </ul>
            {{/each}}
        </div>
    </div>
 </div>
</div>

 
